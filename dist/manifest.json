{
  "manifest_version": 3,
  "name": "Application Extension",
  "version": "0.1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage", "identity", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+U"
      },
      "desciption": "Open Application Tracker Extension"
    }
  }
}
